<!DOCTYPE html>
<html lang="en">

  <%- include('../partials/head') %>

<body>

  <%- include('../partials/navbar') %>

  <div class="login-welcome">
    <h1>Welcome Back!!!</h1>
  </div>
  
  <div class="login-container">
    <!-- sidebar -->
    <div class="login-sidebar">
      <div class="logoPic"><img src="data:image/png;base64,"></div>
    </div>
    <!-- registration container -->
    <div class="login">
      <h1>LOGIN</h1>
      <!-- START FORM -->
      <form action="/user/login" method="POST">
        <!-- fieldset for account info -->
        <fieldset class="account-info">
          <!-- legend for account info -->
          <legend>ACCOUNT INFORMATION</legend>
            <!-- password -->
            <label class="login-label" for="password">PASSWORD</label>
            <input class="login-input" name="password" type="password"/>
            <!-- email -->
            <label class="login-label" for="email">E-MAIL</label>
            <input class="login-input" name="email" type="email"/>
        </fieldset>
        <!-- submit button -->
        <input class="login-button" type="submit" value="LOGIN">
      </form>
    </div>
  </div>
</body>
</html>